<template id="tpl-card-match">
  <article class="card-match">
    <div class="card-match__teams">
      <a data-el="home-link" href="#" class="card-match__team">
        <img data-el="home-logo" src="" alt="" class="card-match__team-logo" loading="lazy" decoding="async">
        <span data-el="home-name" class="card-match__team-name"></span>
        <span data-el="score-home" class="card-match__score">—</span>
      </a>

      <a data-el="away-link" href="#" class="card-match__team">
        <img data-el="away-logo" src="" alt="" class="card-match__team-logo" loading="lazy" decoding="async">
        <span data-el="away-name" class="card-match__team-name"></span>
        <span data-el="score-away" class="card-match__score">—</span>
      </a>

      <a data-el="button" href="#" class="card-match__button" hidden></a>
    </div>

    <div class="card-match__meta">
      <span data-el="date" class="card-match__date"></span>
      <span data-el="status" class="card-match__status"></span>
      <a data-el="league-link" href="#" class="card-match__league"></a>
    </div>
  </article>
</template>